{
  "name": "marcosh/lamphpda-validation",
  "description": "A validation library using Either from marcosh/lamphpda",
  "version": "0.1.0",
  "type": "library",
  "license": "Hippocratic-2.1",
  "authors": [
    {
      "name": "Marco Perone",
      "email": "pasafama@gmail.com"
    }
  ],
  "require": {
    "php": "^8.1",
    "marcosh/lamphpda": "^3.1.1",
    "giorgiosironi/eris": "^1.0.0-rc2",
    "marcosh/lamphpda-optics": "^0.1"
  },
  "autoload": {
    "psr-4": {
      "Marcosh\\LamPHPda\\Validation\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Marcosh\\LamPHPda\\ValidationSpec\\": "spec/"
    }
  },
  "require-dev": {
    "maglnet/composer-require-checker": "^3.8",
    "kahlan/kahlan": "^5.2",
    "squizlabs/php_codesniffer": "^3.10",
    "vimeo/psalm": "^4.30"
  }
}
